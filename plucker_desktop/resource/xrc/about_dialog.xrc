                                            <label>Free Software Foundation's General Public License.</label>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                <border>10</border>
                                <object class="wxStaticText" name="about_dialog_details_tab_url_statictext">
                                    <style>wxALIGN_CENTRE</style>
                                    <label>www.plkr.org</label>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="notebookpage">
                    <label>Plucker Desktop credits</label>
                    <object class="wxPanel" name="about_dialog_desktop_credits_tab">
                        <object class="wxFlexGridSizer">
                            <cols>1</cols>
                            <rows>0</rows>
                            <vgap>0</vgap>
                            <hgap>0</hgap>
                            <growablecols>0</growablecols>
                            <growablerows>0</growablerows>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                <border>5</border>
                                <object class="wxHtmlWindow" name="about_dialog_desktop_credits_tab_htmlwindow">
                                <size platform="unix">400,100</size>
                                <size platform="os2|mac|win">100,100</size>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>  
                <object class="notebookpage">
                    <label>Plucker credits</label>
                    <object class="wxPanel" name="about_dialog_plucker_credits_tab">
                        <object class="wxFlexGridSizer">
                            <cols>1</cols>
                            <rows>0</rows>
                            <vgap>0</vgap>
                            <hgap>0</hgap>
                            <growablecols>0</growablecols>
