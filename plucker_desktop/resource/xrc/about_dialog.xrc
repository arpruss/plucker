                                    <bitmap>../icons_application/application_64x64.xpm</bitmap>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTRE|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                <border>10</border>
                                <object class="wxStaticText" name="about_dialog_details_tab_application_name">
                                    <label>Plucker Desktop</label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_BOTTOM|wxALIGN_CENTER_HORIZONTAL</flag>
