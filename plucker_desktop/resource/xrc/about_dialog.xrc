                                        <object class="wxStaticText" name="about_dialog_details_tab_version_label_statictext">
                                            <label>Version</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTRE|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                        <border>5</border>
                                        <object class="wxStaticText" name="about_dialog_details_tab_version_value_statictext">
                                            <label>1.9.0.0</label>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_BOTTOM|wxALIGN_CENTER_HORIZONTAL</flag>
