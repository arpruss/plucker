                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                <border>10</border>
                                <object class="wxStaticText" name="about_dialog_details_tab_description_statictext">
