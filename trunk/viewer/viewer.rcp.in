FORM ID frmMainTop AT ( 0 0 160 160 )
MENUID menuMainForm
NOFRAME
USABLE
BEGIN
    FORMBITMAP AT (0 0) BITMAP bmpMenu USABLE

    FORMBITMAP AT (15 PREVTOP) BITMAP bmpDbase USABLE
    BUTTON "" frmMainPercentPopup AT (PREVLEFT+17 2 22 9) NOFRAME FONT 0
    LIST "$$MAIN: TOP" "$$MAIN: 10%" "$$MAIN: 20%" "$$MAIN: 30%" 
         "$$MAIN: 40%" "$$MAIN: 50%" "$$MAIN: 60%" "$$MAIN: 70%"
         "$$MAIN: 80%" "$$MAIN: 90%" "$$MAIN: BOTTOM"
         ID frmMainPercentList AT (PREVLEFT PREVTOP MAIN_PERCENT 11)
         NONUSABLE VISIBLEITEMS 11
    POPUPLIST frmMainPercentPopup frmMainPercentList
    FORMBITMAP AT (PREVLEFT+22 0) BITMAP bmpBookmark USABLE
    LIST "" ID frmMainBookmarkList AT (PREVLEFT PREVTOP MAIN_BOOKMARK_LIST 11)
         NONUSABLE VISIBLEITEMS 12
    POPUPLIST bmpBookmark frmMainBookmarkList
    FORMBITMAP AT (PREVLEFT+13 PREVTOP) BITMAP bmpFind USABLE
    FORMBITMAP AT (PREVLEFT+11 PREVTOP) BITMAP bmpAgain USABLE
    FORMBITMAP AT (PREVLEFT+13 PREVTOP) BITMAP bmpAutoscrollDecr USABLE
    FORMBITMAP AT (PREVLEFT+10 PREVTOP) BITMAP bmpAutoscrollStop NONUSABLE
    FORMBITMAP AT (PREVLEFT PREVTOP) BITMAP bmpAutoscrollStart USABLE
    FORMBITMAP AT (PREVLEFT+13 PREVTOP) BITMAP bmpAutoscrollIncr USABLE
    FORMBITMAP AT (PREVLEFT+10 PREVTOP) BITMAP bmpLeft USABLE
    FORMBITMAP AT (PREVLEFT+12 PREVTOP) BITMAP bmpWait NONUSABLE
#ifdef SUPPORT_WORD_LOOKUP
    FORMBITMAP AT (PREVLEFT PREVTOP) BITMAP bmpLookup NONUSABLE
#endif
    FORMBITMAP AT (PREVLEFT PREVTOP) BITMAP bmpHome USABLE
    FORMBITMAP AT (PREVLEFT+13 PREVTOP) BITMAP bmpRight USABLE

#if defined( HAVE_HIRES ) || defined( HAVE_HANDERA_SDK )
    FORMBITMAP AT (0 PREVBOTTOM+1) BITMAP bmpLongBar USABLE
#else
    FORMBITMAP AT (0 PREVBOTTOM+1) BITMAP bmpBar USABLE
#endif

    SCROLLBAR ID frmMainScrollBar AT (153 PREVBOTTOM+2 7 142)
         VALUE 0 MIN 0 MAX 0 PAGESIZE 0

    LIST "$$MAIN: FOLLOW LINK" "$$MAIN: OPEN IMAGE"
         ID frmMainImageDialog AT (0 0 MAIN_IMAGE_DIALOG 11)
         NONUSABLE VISIBLEITEMS 2
    POPUPLIST frmMainImageDialogPopup frmMainImageDialog
END

