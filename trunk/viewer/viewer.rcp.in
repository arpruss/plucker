MENU menuMainForm
BEGIN
    PULLDOWN "$$GO"
    BEGIN
        MENUITEM "$$LIBRARY..."     mGoLibrary          "$$SC LIBRARY"
        MENUITEM "$$DELETE DOC..."  mGoDeleteDocument   "$$SC DELETE DOC"
        MENUITEM SEPARATOR
        MENUITEM "$$HOME"           mGoHome             "$$SC HOME"
        MENUITEM "$$BACK"           mGoBack             "$$SC BACK"
        MENUITEM "$$FORWARD"        mGoForward          "$$SC FORWARD"
        MENUITEM SEPARATOR
        MENUITEM "$$TOP"            mGoTop              "$$SC TOP"
        MENUITEM "$$BOTTOM"         mGoBottom           "$$SC BOTTOM"
        MENUITEM SEPARATOR
        MENUITEM "$$FIND..."        mGoSearch           "$$SC FIND"
        MENUITEM "$$FIND AGAIN"     mGoSearchAgain      "$$SC FIND AGAIN"
        MENUITEM SEPARATOR
        MENUITEM "$$ADD BOOKMARK..."   mGoAddBookmark   "$$SC ADD BOOKMARK"
        MENUITEM "$$BOOKMARKS..."   mGoBookmarks        "$$SC BOOKMARKS"
    END

    PULLDOWN "$$VIEW"
    BEGIN
        MENUITEM "$$DETAILS..."         mViewDetails            "$$SC DETAILS"
        MENUITEM SEPARATOR
        MENUITEM "$$START AUTOSCROLL"   mViewAutoscrollStart    "$$SC START AUTOSCROLL"
        MENUITEM "$$STOP AUTOSCROLL"    mViewAutoscrollStop     "$$SC STOP AUTOSCROLL"
        MENUITEM "$$AUTOSCROLL FASTER"  mViewAutoscrollIncr     "$$SC AUTOSCROLL FASTER"
        MENUITEM "$$AUTOSCROLL SLOWER"  mViewAutoscrollDecr     "$$SC AUTOSCROLL SLOWER"
        MENUITEM SEPARATOR
        MENUITEM "$$TOGGLE FULLSCREEN"  mViewToggleFullscreen   "$$SC TOGGLE FULLSCREEN"
        MENUITEM SEPARATOR
        MENUITEM "$$COPY TO MEMO"       mViewCopyToMemo         "$$SC COPY TO MEMO"
#ifdef SUPPORT_WORD_LOOKUP
        MENUITEM "$$LOOKUP WORD"        mGoLookup               "$$SC LOOKUP WORD"
#endif    
    END

    PULLDOWN "$$OPTIONS"
    BEGIN
        MENUITEM "$$SELECT FONT..."     mOptionsFont     "$$SC SELECT FONT"
        MENUITEM "$$SET KEYBOARD/GRAFFITI..." mOptionsKeyboard  "$$SC SET KEYBOARD/GRAFFITI"
        MENUITEM "$$PREFERENCES..."     mOptionsPref     "$$SC PREFERENCES"
        MENUITEM SEPARATOR
        MENUITEM "$$CONTACT DEVELOPERS" mOptionsContact
        MENUITEM SEPARATOR
        MENUITEM "$$ABOUT PLUCKER"      mOptionsAbout
    END
END


