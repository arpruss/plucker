<?xml version="1.0" encoding="ISO-8859-1"?>

<resource xmlns="http://www.wxwidgets.org/wxxrc" version="2.5.3.0">

<object class="wxDialog" name="channel_dialog">
    <style>wxCAPTION|wxSYSTEM_MENU|wxRESIZE_BORDER</style>
    <title>Configure Channel</title>
    <object class="wxFlexGridSizer">
        <cols>1</cols>
        <rows>0</rows>
        <vgap>0</vgap>
        <hgap>0</hgap>
        <growablecols>0</growablecols>
        <growablerows>1</growablerows>
        <object class="sizeritem">
            <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
            <border>5</border>
            <object class="wxFlexGridSizer">
                <cols>2</cols>
                <rows>0</rows>
                <vgap>0</vgap>
                <hgap>0</hgap>
                <growablecols>0,1</growablecols>
                <growablerows>0</growablerows>
                <object class="sizeritem">
                    <flag>wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                    <border>5</border>
                    <object class="wxStaticText" name="channel_dialog_statictext">
                        <label>Channel name:</label>
                    </object>
                </object>
                <object class="sizeritem">
                    <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                    <border>5</border>
                    <object class="wxTextCtrl" name="channel_dialog_textctrl">
                        <size>250,-1</size>
                        <value></value>
                    </object>
                </object>
            </object>
        </object>
        <object class="sizeritem">
            <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
            <border>5</border>
            <object class="wxNotebook" name="channel_dialog_notebook">
                <style></style>
                <usenotebooksizer>1</usenotebooksizer>
                <object class="notebookpage">
                    <label>Starting page</label>
                    <object class="wxPanel" name="channel_dialog_start_tab">
                        <object class="wxBoxSizer">
                            <orient>wxVERTICAL</orient>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>Start with</label>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxFlexGridSizer">
                                            <cols>0</cols>
                                            <rows>3</rows>
                                            <vgap>0</vgap>
                                            <hgap>0</hgap>
                                            <growablecols>1</growablecols>
                                            <growablerows>0,1,2</growablerows>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="channel_dialog_start_tab_url_radiobutton">
                                                    <style>wxRB_GROUP</style>
                                                    <label>A website URL:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxTextCtrl" name="channel_dialog_start_tab_url_textctrl">
                                                    <size>240,-1</size>
                                                    <value>http://</value>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxButton" name="channel_dialog_start_tab_url_button">
                                                    <label>Browse showcase...</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="channel_dialog_start_tab_file_radiobutton">
                                                    <label>A local file:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxTextCtrl" name="channel_dialog_start_tab_file_textctrl">
                                                    <size>240,-1</size>
                                                    <value>file://</value>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                                <border>5</border>
                                                <object class="wxBoxSizer">
                                                    <orient>wxHORIZONTAL</orient>
                                                    <object class="sizeritem">
                                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                        <border>5</border>
                                                        <object class="wxButton" name="channel_dialog_start_tab_file_button">
                                                            <label>Choose file...</label>
                                                        </object>
                                                    </object>
                                                    <object class="sizeritem">
                                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                        <border>5</border>
                                                        <object class="wxButton" name="channel_dialog_start_tab_edit_file_button">
                                                            <label>Edit file...</label>
                                                        </object>
                                                    </object>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="channel_dialog_start_tab_links_radiobutton">
                                                    <label>A list of my links:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxButton" name="channel_dialog_start_tab_links_button">
                                                    <label>Edit my list of links with HTML editor...</label>
                                                </object>
                                            </object>
                                            <object class="spacer">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                    <size>5,5</size>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="notebookpage">
                    <label>Spidering</label>
                    <object class="wxPanel" name="channel_dialog_spidering_tab">
                        <object class="wxBoxSizer">
                            <orient>wxVERTICAL</orient>
							<object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>Timeout</label>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxBOTTOM</flag>
                                        <border>5</border>
                                        <object class="wxBoxSizer">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTER|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxCheckBox" name="channel_dialog_spidering_tab_retrieval_timeout_checkbox">
                                                    <label>Stop spidering if can't connect to the server within</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTER|wxLEFT|wxRIGHT</flag>
                                                <border>5</border>
                                                <object class="wxSpinCtrl" name="channel_dialog_spidering_tab_retrieval_timeout_spinctrl">
                                                    <size>100,-1</size>
                                                    <min>3</min>
                                                    <max>360</max>
                                                    <value>0</value>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTER|wxLEFT|wxRIGHT</flag>
                                                <border>5</border>
                                                <object class="wxStaticText" name="channel_dialog_spidering_tab_retrieval_timeout_statictext">
                                                    <label>seconds</label>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>Retrieval order</label>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                        <border>5</border>
                                        <object class="wxFlexGridSizer">
                                            <cols>2</cols>
                                            <rows>0</rows>
                                            <vgap>0</vgap>
                                            <hgap>0</hgap>
                                            <growablecols>1</growablecols>
                                            <growablerows>0</growablerows>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxStaticText" name="channel_dialog_spidering_tab_depth_first_statictext">
                                                    <label>Retrieve files by following links:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxChoice" name="channel_dialog_spidering_tab_depth_first_choice">
                                                    <size>300,-1</size>
                                                    <content>
                                                        <item>Breadth first</item>
                                                        <item>Depth first</item>
                                                    </content>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>Spidering status</label>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                        <border>5</border>
                                        <object class="wxFlexGridSizer">
                                            <cols>2</cols>
                                            <rows>0</rows>
                                            <vgap>0</vgap>
                                            <hgap>0</hgap>
                                            <growablecols>1</growablecols>
                                            <growablerows>0</growablerows>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxStaticText" name="channel_dialog_spidering_tab_verbosity_statictext">
                                                    <label>Progress message detail level:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxChoice" name="channel_dialog_spidering_tab_verbosity_choice">
                                                    <size>300,-1</size>
                                                    <content>
                                                        <item>Minimum (no progress information, only error messages)</item>
                                                        <item>Medium (page being spidered, pages remaining in queue)</item>
                                                        <item>Maximum (full information, most useful for debugging)</item>
                                                    </content>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxFlexGridSizer">
                                            <cols>3</cols>
                                            <rows>0</rows>
                                            <vgap>0</vgap>
                                            <hgap>5</hgap>
                                            <growablecols>1</growablecols>
                                            <growablerows>0</growablerows>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                                <border>10</border>
                                                <object class="wxCheckBox" name="channel_dialog_spidering_tab_status_line_length_checkbox">
                                                    <label>Truncate URLs displayed in the progress window to:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxLEFT|wxRIGHT</flag>
                                                <border>5</border>
                                                <object class="wxSpinCtrl" name="channel_dialog_spidering_tab_status_line_length_spinctrl">
                                                    <size>100,-1</size>
                                                    <min>1</min>
                                                    <max>1000000</max>
                                                    <value>0</value>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxRIGHT|wxTOP|wxBOTTOM</flag>
                                                <border>5</border>
                                                <object class="wxStaticText" name="channel_dialog_spidering_tab_status_line_length_statictext">
                                                    <label>letters</label>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>Referrers and browser identification</label>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxFlexGridSizer">
                                            <cols>2</cols>
                                            <rows>0</rows>
                                            <vgap>0</vgap>
                                            <hgap>5</hgap>
                                            <growablecols>1</growablecols>
                                            <growablerows>0</growablerows>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                                <border>10</border>
                                                <object class="wxCheckBox" name="channel_dialog_spidering_tab_referrer_checkbox">
                                                    <label>Specify a referrer URL:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                                <border>5</border>
                                                <object class="wxTextCtrl" name="channel_dialog_spidering_tab_referrer_textctrl">
                                                    <size>225,-1</size>
                                                    <value></value>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxStaticText" name="channel_dialog_spidering_tab_user_agent_statictext">
                                            <label>When retrieving files from this channel's web server, identify as:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxRIGHT</flag>
                                        <border>5</border>
                                        <object class="wxFlexGridSizer">
                                            <cols>3</cols>
                                            <rows>0</rows>
                                            <vgap>0</vgap>
                                            <hgap>5</hgap>
                                            <growablecols>2</growablecols>
                                            <growablerows>0,1</growablerows>
                                            <object class="spacer">
                                                <flag>wxALIGN_CENTRE</flag>
                                                    <size>20,20</size>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                                                <object class="wxRadioButton" name="channel_dialog_spidering_tab_user_agent_browser_radiobutton">
                                                    <style>wxRB_GROUP</style>
                                                    <label>This browser type:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                                <border>5</border>
                                                <object class="wxChoice" name="channel_dialog_spidering_tab_user_agent_browser_choice">
                                                    <size>100,-1</size>
                                                    <content>
                                                        <item>Plucker (recommended)</item>
                                                        <item>AvantGo</item>
                                                    </content>
                                                </object>
                                            </object>
                                            <object class="spacer">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                    <size>20,20</size>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <object class="wxRadioButton" name="channel_dialog_spidering_tab_user_agent_custom_radiobutton">
                                                    <label>This custom user-agent:</label>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxTOP|wxBOTTOM</flag>
                                                <border>5</border>
                                                <object class="wxTextCtrl" name="channel_dialog_spidering_tab_user_agent_custom_textctrl">
                                                    <size>80,-1</size>
                                                    <value></value>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="notebookpage">
                    <label>Limits</label>
                    <object class="wxPanel" name="channel_dialog_limits_tab">
                        <object class="wxBoxSizer">
                            <orient>wxVERTICAL</orient>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxHORIZONTAL</orient>
                                    <label>Maximum depth</label>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTRE|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxStaticText" name="channel_dialog_limits_tab_maxdepth1_statictext">
                                            <label>Retreive linked pages to a depth of</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTRE|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxSpinCtrl" name="channel_dialog_limits_tab_maxdepth_spinctrl">
                                            <size>70,-1</size>
                                            <min>1</min>
                                            <max>1000000</max>
                                            <value>2</value>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTRE|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxStaticText" name="channel_dialog_limits_tab_maxdepth2_statictext">
                                            <label>from the starting page (1=retrieve starting page only)</label>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>'Stay on host' / 'Stay on domain' filters</label>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxCheckBox" name="channel_dialog_limits_tab_stayonhost_stayondomain_checkbox">
                                            <label>Ignore links to a server that is different from starting page's server</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                        <border>5</border>
                                        <object class="wxBoxSizer">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="spacer">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                    <size>10,10</size>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxLEFT|wxRIGHT</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="channel_dialog_limits_tab_stayonhost_radiobutton">
                                                    <style>wxRB_GROUP</style>
                                                    <label>Limit to the exact server only: if started on www.plkr.org, only allow www.plkr.org</label>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                        <border>5</border>
                                        <object class="wxBoxSizer">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="spacer">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                    <size>10,10</size>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTRE|wxLEFT|wxRIGHT</flag>
                                                <border>5</border>
                                                <object class="wxRadioButton" name="channel_dialog_limits_tab_stayondomain_radiobutton">
                                                    <label>Only restrict to domain: if started on www.plkr.org, allow www.plkr.org and www2.plkr.org</label>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>'URL pattern' filter</label>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxCheckBox" name="channel_dialog_limits_tab_url_pattern_checkbox">
                                            <label>Only follow URLs that match this regular expression pattern:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL</flag>
                                        <border>5</border>
                                        <object class="wxFlexGridSizer">
                                            <cols>2</cols>
                                            <rows>0</rows>
                                            <vgap>0</vgap>
                                            <hgap>0</hgap>
                                            <growablecols>1</growablecols>
                                            <growablerows>0</growablerows>
                                            <object class="spacer">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                    <size>10,10</size>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxTextCtrl" name="channel_dialog_limits_tab_url_pattern_textctrl">
                                                    <size>300,-1</size>
                                                    <value></value>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT|wxBOTTOM</flag>
                                <border>5</border>
                                <object class="wxStaticBoxSizer">
                                    <orient>wxVERTICAL</orient>
                                    <label>Channel-specific inclusion/exclusion list filter</label>
                                    <object class="sizeritem">
                                        <flag>wxGROW|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                        <border>5</border>
                                        <object class="wxCheckBox" name="channel_dialog_limits_tab_exclusion_checkbox">
                                            <label>In addition to the shared inclusion/exclusion list, also follow/ignore links pointing to these file types or servers:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <flag>wxALIGN_CENTER_VERTICAL</flag>
                                        <border>5</border>
                                        <object class="wxBoxSizer">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="spacer">
                                                <flag>wxALIGN_CENTRE|wxALL</flag>
                                                <border>5</border>
                                                    <size>10,10</size>
                                            </object>
                                            <object class="sizeritem">
                                                <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>5</border>
                                                <object class="wxButton" name="channel_dialog_limits_tab_exclusion_button">
                                                    <label>Edit channel-specific inclusion/exclusion list...</label>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
                <object class="notebookpage">
                    <label>Formatting</label>
                    <object class="wxPanel" name="channel_dialog_formatting_tab">
                        <object class="wxBoxSizer">
                            <orient>wxVERTICAL</orient>
                            <object class="sizeritem">
