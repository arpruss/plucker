                                                <object class="wxButton" name="channel_dialog_output_tab_custom_small_icon_button">
                                                    <label>Choose file...</label>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
